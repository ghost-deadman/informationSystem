<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.informationSystem.mapper.ProjectBudgetMapper">

    <resultMap id="BaseResultMap" type="com.example.informationSystem.entity.ProjectBudget">
        <id column="project_budget_id" property="projectBudgetId"/>
        <result column="money" property="money"/>
        <result column="deleted" property="deleted"/>
        <result column="opinion_id" property="opinionId"/>
        <result column="date" property="date"/>
        <result column="describes" property="describes"/>
        <result column="status" property="status" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, url, path, component, menu_name, icon, keepAlive, requireAuth, enabled
    </sql>

    <select id="selectPageVo" resultType="com.example.informationSystem.entity.ProjectBudget">
        select * from subject where name = #{name}
    </select>

</mapper>
